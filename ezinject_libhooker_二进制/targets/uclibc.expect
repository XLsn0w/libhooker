#!/usr/bin/env expect
spawn ssh root@192.168.1.1
expect -- "root@dlinkrouter:~# "
send -- "kill -9 \$(pidof ezinject)\n"
send -- "kill -9 \$(pidof target)\n"
send -- "/tmp/target &\n"
send -- "sleep 2\n"
send -- "/tmp/ezinject \$(pidof target) /tmp/libdummy.so 1 2 3 4\n"
send -- "fg %1\n"
interact